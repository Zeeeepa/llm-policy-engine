{
  "name": "@llm-devops/policy-engine",
  "version": "1.0.0",
  "description": "Declarative policy layer for cost, security, and compliance enforcement in LLM DevOps ecosystem",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "llm-policy": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc && tsc-alias",
    "build:proto": "node scripts/build-proto.js",
    "dev": "tsx watch src/index.ts",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:integration": "jest --testPathPattern=integration",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install",
    "start": "node dist/index.js",
    "start:api": "node dist/api/server.js",
    "start:grpc": "node dist/grpc/server.js",
    "db:migrate": "node dist/db/migrate.js",
    "db:seed": "node dist/db/seed.js"
  },
  "keywords": [
    "llm",
    "policy",
    "governance",
    "compliance",
    "cost-management",
    "security",
    "opa",
    "policy-engine",
    "llm-devops"
  ],
  "author": "LLM DevOps Team",
  "license": "Apache-2.0",
  "dependencies": {
    "@grpc/grpc-js": "^1.10.1",
    "@grpc/proto-loader": "^0.7.10",
    "@opentelemetry/api": "^1.8.0",
    "@opentelemetry/exporter-jaeger": "^2.2.0",
    "@opentelemetry/exporter-prometheus": "^0.50.0",
    "@opentelemetry/instrumentation-express": "^0.39.0",
    "@opentelemetry/instrumentation-http": "^0.50.0",
    "@opentelemetry/instrumentation-pg": "^0.41.0",
    "@opentelemetry/instrumentation-redis-4": "^0.40.0",
    "@opentelemetry/sdk-metrics": "^1.22.0",
    "@opentelemetry/sdk-trace-node": "^1.22.0",
    "@types/express-rate-limit": "^6.0.2",
    "ajv": "^8.12.0",
    "ajv-formats": "^3.0.1",
    "axios": "^1.13.2",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "commander": "^12.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "express-rate-limit": "^7.2.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "joi": "^17.12.2",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "luxon": "^3.4.4",
    "pg": "^8.11.3",
    "pino": "^8.19.0",
    "pino-pretty": "^11.0.0",
    "prom-client": "^15.1.0",
    "uuid": "^9.0.1",
    "winston": "^3.12.0",
    "yaml": "^2.4.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/body-parser": "^1.19.5",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/lodash": "^4.17.0",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.11.30",
    "@types/pg": "^8.11.4",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "tsx": "^4.7.1",
    "typescript": "^5.4.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
